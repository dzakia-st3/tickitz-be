{
    "version": 2,
    "builds" : [
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/",
            "src": "/movie/", "dest": "/movieRoutes.js",
            "src": "/movie/", "methods": ["GET"], "dest": "/movieRoutes.js"
        }
    ]
  }
  